# FortisPay API Configuration
# Get these credentials from your FortisPay account at https://fortispay.com/developers/

# FortisPay API URL (use sandbox for testing, production for live)
VITE_FORTIS_API_URL=https://api.fortispay.com/v2
# For sandbox testing, use: https://sandbox.fortispay.com/v2

# FortisPay User Credentials
VITE_FORTIS_USER_ID=your_user_id_here
VITE_FORTIS_USER_API_KEY=your_user_api_key_here

# FortisPay Location ID (from your FortisPay account)
VITE_FORTIS_LOCATION_ID=your_location_id_here

# FortisPay Product Transaction ID for ACH payments
VITE_FORTIS_PRODUCT_TRANSACTION_ID_ACH=your_ach_product_transaction_id_here

# Payment Processor Selection
# Options: "authorize_net" or "fortispay"
VITE_ACH_PAYMENT_PROCESSOR=fortispay
